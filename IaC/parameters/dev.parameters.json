{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "East US"
    },
    "environmentName": {
      "value": "dev"
    },
    "appNamePrefix": {
      "value": "TDC"
    },
    "sqlAdminLogin": {
      "value": "tdcadmin"
    },
    "sqlAdminPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{key-vault-rg}/providers/Microsoft.KeyVault/vaults/{key-vault-name}"
        },
        "secretName": "sql-admin-password"
      }
    }
  }
}